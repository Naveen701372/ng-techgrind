!!!5
html(lang="en", ng-app="TechGrindApp")
	head
		meta(charset="utf-8")
		link(rel="shortcut icon", href="img/tgfavicon-gear.ico")
		title TechGrind &middot; Asia's Startup Community

		//- for angular UI, from http://www.bootstrapcdn.com/
		//- <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" rel="stylesheet">
		link(rel="stylesheet", href="css/bootstrap-combined.min.css")

		link(rel="stylesheet", href="css/app.css")
	body(ng-controller="AppCtrl")
		heading
			h1 TechGrind
		nav(ng-include="'partials/menu.html'")
		section#main(ng-view)
		footer
			ul
				li 123
				li 1234
				li 12345
		//- In production use:
		//- script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.0.6/angular.min.js")
		script(src="lib/angular/angular.js")
		//- from https://raw.github.com/angular-ui/bootstrap/gh-pages/ui-bootstrap-tpls-0.3.0.js
		script(src="lib/angular/ui-bootstrap-tpls-0.3.0.js")

		script(src="js/app.js")
		script(src="js/services.js")
		script(src="js/controllers.js")
		script(src="js/filters.js")
		script(src="js/directives.js")
		if settings.env == 'test'
			script(src="test/lib/angular/angular-mocks.js")
			script(src="test/e2e/backend-mocks.js")
